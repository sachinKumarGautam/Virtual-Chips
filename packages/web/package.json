{
    "name": "web",
    "version": "1.0.0",
    "description": "web package.json",
    "license": "MIT",
    "scripts": {
        "client": "cd client && yarn start",
        "client-build": "cd client && yarn build",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "build": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client-build\""
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "concurrently": "^4.1.0"
    }
}
